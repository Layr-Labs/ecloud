{
  "name": "@layr-labs/ecloud-sdk",
  "version": "development",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "tools",
    "VERSION",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./compute": {
      "types": "./dist/compute.d.ts",
      "import": "./dist/compute.js",
      "require": "./dist/compute.cjs"
    },
    "./billing": {
      "types": "./dist/billing.d.ts",
      "import": "./dist/billing.js",
      "require": "./dist/billing.cjs"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.js",
      "require": "./dist/browser.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:dev": "BUILD_TYPE=dev tsup",
    "prepublishOnly": "cp ../../README.md .",
    "lint": "eslint .",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.10.1",
    "@napi-rs/keyring": "^1.0.5",
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "dockerode": "^4.0.9",
    "form-data": "^4.0.5",
    "handlebars": "^4.7.8",
    "jose": "^6.1.2",
    "js-yaml": "^4.1.1",
    "node-forge": "^1.3.2",
    "posthog-node": "^5.17.2",
    "viem": "^2.38.6"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.45",
    "@types/form-data": "^2.5.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node-forge": "^1.3.14",
    "@types/react": "^18.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
